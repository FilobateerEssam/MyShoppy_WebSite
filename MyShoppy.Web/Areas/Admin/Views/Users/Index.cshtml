@model IEnumerable<ApplicationUser>




@{
    ViewBag.PageTitle = "Users Management";
    ViewBag.CardTitle = "Show All Users";
}

<table class="table table-hover" width="100%">

    <tr>
        <td>Name</td>
        <td>Email</td>
        <td>Phone Number</td>
        <td>
            <a href="/Identity/Account/Register" class="btn btn-primary">Create User</a>

        </td>
    </tr>
    @foreach (var item in Model)
    {

        <tr>
            <td>@item.FullName</td>
            <td>@item.Email</td>
            <td>@item.PhoneNumber</td>
            <td>
                <a class="btn btn-success" asp-action="LockUnlock">
                    <i class="fas fa-lock-open"></i>
                </a>
                <a asp-controller="Category" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
            </td>

        </tr>

    }
</table>



@section Scripts {
    <script src="~/js/products.js"></script>
}